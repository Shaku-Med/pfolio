    },
    "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
    "protocol_handlers": [
      {
        "protocol": "web+portfolio",
        "url": "/handle?url=%s"
      }
    ],
    "file_handlers": [
      {
        "action": "/contact",
        "accept": {
          "application/pdf": [".pdf"],
          "text/plain": [".txt"],
          "image/*": [".jpg", ".jpeg", ".png", ".gif", ".webp"]
        }
      }
    ],
    "share_target": {
      "action": "/contact",
      "method": "POST",
      "enctype": "multipart/form-data",
      "params": {
        "title": "title",
        "text": "text",
        "url": "url",
        "files": [
          {
            "name": "files",
            "accept": ["application/pdf", "text/plain", "image/*"]
          }
        ]
      }
    },
    "background_color": "#000"
